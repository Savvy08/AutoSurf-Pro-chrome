{
  "manifest_version": 3,
  "name": "AutoSurf Pro",
  "version": "1.7",
  "description": "Автоматический переход по сайтам с настройками",
  "permissions": ["storage", "tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
