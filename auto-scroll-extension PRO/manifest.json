{
  "manifest_version": 3,
  "name": "AutoSurf Pro",
  "version": "1.6",
  "description": "Автосёрфинг сайтов с прокруткой, переходами и исключениями",
  "permissions": ["tabs", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
